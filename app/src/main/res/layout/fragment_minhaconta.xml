<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/containerMinhaConta"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pokeBlueFundo"
    tools:context=".fragments.minhaContaFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView36"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="32dp"
            android:text="Minha Conta"
            android:textSize="18sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:text="Olá,"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView9"
            app:layout_constraintEnd_toStartOf="@+id/textViewUsuario"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView36"
            app:layout_constraintVertical_bias="0.473" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="127dp"
            android:layout_height="111dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="96dp"
            android:layout_marginEnd="32dp"
            android:src="@drawable/pokemontrainer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

        <com.google.android.material.chip.Chip
            android:id="@+id/button12"
            android:layout_width="132dp"
            android:layout_height="55dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="76dp"
            android:backgroundTint="#E6E3350D"
            android:text="Mudar Imagem"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/chipMeuPedido"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.023" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chipMeuPedido"
            android:layout_width="132dp"
            android:layout_height="55dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="72dp"
            android:text="Meus Pedidos"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:chipBackgroundColor="#E6E3350D"
            app:layout_constraintEnd_toStartOf="@+id/chipEditaEndereco"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button12"
            tools:text="Meus Pedidos" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chipEditaEndereco"
            android:layout_width="132dp"
            android:layout_height="55dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="39dp"
            android:text="Editar Endereço"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:chipBackgroundColor="#E6E3350D"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/chipMeuPedido"
            app:layout_constraintTop_toBottomOf="@+id/chipMeuPerfil" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chipMeuPerfil"
            android:layout_width="132dp"
            android:layout_height="55dp"
            android:layout_marginStart="72dp"
            android:layout_marginTop="137dp"
            android:layout_marginEnd="39dp"
            android:text="Meu Perfil"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:chipBackgroundColor="#E6E3350D"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button12"
            app:layout_constraintTop_toBottomOf="@+id/chipLogin" />

        <com.google.android.material.chip.Chip
            android:id="@+id/imageViewLogoff"
            android:layout_width="132dp"
            android:layout_height="55dp"
            android:layout_marginStart="139dp"
            android:layout_marginEnd="140dp"
            android:layout_marginBottom="57dp"
            android:text="Sair"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:chipBackgroundColor="#E6E3350D"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chipLogin"
            android:layout_width="132dp"
            android:layout_height="51dp"
            android:layout_marginStart="81dp"
            android:layout_marginTop="96dp"
            android:layout_marginEnd="39dp"
            android:text="Login"
            android:textAlignment="center"
            android:textColor="@color/white"
            app:chipBackgroundColor="@color/pokeBlueBotao"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView9"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

        <TextView
            android:id="@+id/textViewUsuario"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="257dp"
            android:text="TextView"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView38"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="244dp"
            android:layout_height="40dp"
            android:layout_marginStart="83dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="84dp"
            android:src="@drawable/minhacontaperfil"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>